{
    "mcpServers": {
        
        "airbnb": {
            "command": "npx",
            "args": [
                "-y",
                "@openbnb/mcp-server-airbnb",
                "--ignore-robots-txt"
            ]
        },
        "fetch": {
            "command": "npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@smithery-ai/fetch",
                "--key",
                "ba2d4989-2d87-41b5-9753-04c10f47adac"
            ]
        },
        "tavily-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "tavily-mcp@0.1.2"
            ],
            "env": {
                "TAVILY_API_KEY": "your-api-key-here"
            }
        },
        "google-calender": {
            "url": "http://localhost:5678/mcp/my-calendar/sse",
            "transport": "sse"
        },
        "serp": {
            "url": "http://localhost:5678/mcp/tavily/sse",
            "transport": "sse"
        }
    }
}