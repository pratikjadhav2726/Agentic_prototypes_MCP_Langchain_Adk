{
    "mcpServers": {
        "math": {
            "command": "python",
            "args": [
                "math_server.py"
            ],
            "transport": "stdio"
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ],
            "env": {
                "DISPLAY": ":1"
            }
        },
        "airbnb": {
            "command": "npx",
            "args": [
                "-y",
                "@openbnb/mcp-server-airbnb",
                "--ignore-robots-txt"
            ]
        },
        "fetch": {
            "command": "npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@smithery-ai/fetch",
                "--key",
                "ba2d4989-2d87-41b5-9753-04c10f47adac"
            ]
        }
    }
}