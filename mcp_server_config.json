{
    "math": {
        "command": "python",
        "args": ["math_server.py"],
        "transport": "stdio"
    },
    "playwright": {
        "command": "npx",
        "args": ["@playwright/mcp@latest"],
        "env": {
            "DISPLAY": ":1"
        }
    },
    "airbnb": {
        "command": "npx",
        "args": [
            "-y",
            "@openbnb/mcp-server-airbnb"
        ]
    }
}